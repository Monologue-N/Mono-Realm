#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Clarisse/Developer/Notes/node_modules/.pnpm/ejs@3.1.6/node_modules/ejs/bin/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/ejs@3.1.6/node_modules/ejs/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/ejs@3.1.6/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/node_modules:/Users/Clarisse/Developer/Notes/node_modules:/Users/Clarisse/Developer/node_modules:/Users/Clarisse/node_modules:/Users/node_modules:/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/hexo-renderer-ejs@2.0.0/node_modules/hexo-renderer-ejs/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/hexo-renderer-ejs@2.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/ejs@3.1.6/node_modules/ejs/bin/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/ejs@3.1.6/node_modules/ejs/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/ejs@3.1.6/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/node_modules:/Users/Clarisse/Developer/Notes/node_modules:/Users/Clarisse/Developer/node_modules:/Users/Clarisse/node_modules:/Users/node_modules:/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/hexo-renderer-ejs@2.0.0/node_modules/hexo-renderer-ejs/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/hexo-renderer-ejs@2.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../ejs@3.1.6/node_modules/ejs/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../ejs@3.1.6/node_modules/ejs/bin/cli.js" "$@"
fi
