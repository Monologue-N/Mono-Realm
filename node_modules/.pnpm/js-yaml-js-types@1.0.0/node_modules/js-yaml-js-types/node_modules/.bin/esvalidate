#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Clarisse/Developer/Notes/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/esprima@4.0.1/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/node_modules:/Users/Clarisse/Developer/Notes/node_modules:/Users/Clarisse/Developer/node_modules:/Users/Clarisse/node_modules:/Users/node_modules:/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/js-yaml-js-types@1.0.0/node_modules/js-yaml-js-types/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/js-yaml-js-types@1.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/esprima@4.0.1/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/node_modules:/Users/Clarisse/Developer/Notes/node_modules:/Users/Clarisse/Developer/node_modules:/Users/Clarisse/node_modules:/Users/node_modules:/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/js-yaml-js-types@1.0.0/node_modules/js-yaml-js-types/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/js-yaml-js-types@1.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esprima@4.0.1/node_modules/esprima/bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../../../../../esprima@4.0.1/node_modules/esprima/bin/esvalidate.js" "$@"
fi
