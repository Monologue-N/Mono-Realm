#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Clarisse/Developer/Notes/node_modules/.pnpm/nunjucks@3.2.3/node_modules/nunjucks/bin/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/nunjucks@3.2.3/node_modules/nunjucks/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/nunjucks@3.2.3/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/node_modules:/Users/Clarisse/Developer/Notes/node_modules:/Users/Clarisse/Developer/node_modules:/Users/Clarisse/node_modules:/Users/node_modules:/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/hexo@6.1.0/node_modules/hexo/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/hexo@6.1.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/nunjucks@3.2.3/node_modules/nunjucks/bin/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/nunjucks@3.2.3/node_modules/nunjucks/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/nunjucks@3.2.3/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/node_modules:/Users/Clarisse/Developer/Notes/node_modules:/Users/Clarisse/Developer/node_modules:/Users/Clarisse/node_modules:/Users/node_modules:/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/hexo@6.1.0/node_modules/hexo/node_modules:/Users/Clarisse/Developer/Notes/node_modules/.pnpm/hexo@6.1.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nunjucks@3.2.3/node_modules/nunjucks/bin/precompile" "$@"
else
  exec node  "$basedir/../../../../../nunjucks@3.2.3/node_modules/nunjucks/bin/precompile" "$@"
fi
